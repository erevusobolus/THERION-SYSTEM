{
    "name": "kinesis-v2-website",
    "version": "2.0.0",
    "description": "KINESIS V2 - Professional Leadership Development and Team Coaching. Next.js 14 website for Danai Leventakou's coaching services.",
    "keywords": [
        "leadership-development",
        "team-coaching",
        "emotional-intelligence",
        "communication-training",
        "decision-making",
        "psychology",
        "somatics",
        "learning-sciences",
        "next.js",
        "react",
        "typescript",
        "tailwindcss",
        "framer-motion"
    ],
    "homepage": "https://kinesisnow.com",
    "author": {
        "name": "Danai Leventakou",
        "email": "hello@kinesisnow.com",
        "url": "https://kinesisnow.com"
    },
    "license": "MIT",
    "private": true,
    "scripts": {
        "dev": "next dev -p 3001",
        "dev:debug": "NODE_OPTIONS='--inspect' next dev -p 3001",
        "dev:turbo": "next dev --turbo -p 3001",
        "build": "next build",
        "start": "next start -p 3001",
        "export": "next build && next export",
        "lint": "next lint",
        "lint:fix": "next lint --fix",
        "type-check": "tsc --noEmit",
        "postbuild": "next-sitemap",
        "clean": "rm -rf .next out",
        "analyze": "ANALYZE=true npm run build"
    },
    "dependencies": {
        "@heroicons/react": "^2.2.0",
        "@tailwindcss/typography": "^0.5.0",
        "@types/node": "^20.12.0",
        "@types/nodemailer": "^7.0.1",
        "@types/react": "^18.3.0",
        "@types/react-dom": "^18.3.0",
        "autoprefixer": "^10.4.0",
        "clsx": "^2.1.0",
        "eslint": "^8.57.0",
        "eslint-config-next": "^14.2.0",
        "framer-motion": "^11.18.2",
        "howler": "^2.2.4",
        "lucide-react": "^0.378.0",
        "next": "^14.2.32",
        "nodemailer": "^7.0.6",
        "postcss": "^8.4.0",
        "react": "^18.3.0",
        "react-dom": "^18.3.0",
        "tailwindcss": "^3.4.0",
        "typescript": "^5.4.0"
    },
    "devDependencies": {
        "@types/howler": "^2.2.12",
        "next-sitemap": "^4.2.3"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=8.0.0"
    },
    "browserslist": [
        "defaults",
        "not IE 11"
    ]
}